//definicija funkcije - pazi, definirati mora biti izven setup in main funkcije
//int je oznaka za cela števila
int sestevanje(int a, int b){
  return a + b;

}

void setup() {
  // tu se koda izvede le enkrat:
  Serial.begin(9600);
  Serial.println(sestevanje(5,12));
  }

void loop() {
  // tu se koda ves čas izvaja:

}

